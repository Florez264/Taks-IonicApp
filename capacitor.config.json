{
  "appId": "com.tuempresa.franquiciaapp",
  "appName": "FranquiciaApp",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": true,
      "backgroundColor": "#ffffff",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP"
    },
    "CapacitorHttp": {
      "enabled": true
    }
  },
  "android": {
    "allowMixedContent": true
  },
  "server": {
    "allowNavigation": [
      "*"
    ]
  }
}
